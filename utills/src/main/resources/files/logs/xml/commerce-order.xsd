	<xsd:simpleType name="Integer4Type">
		<xsd:annotation>
			<xsd:documentation>Defines a number with 4 digits.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:integer">
			<xsd:totalDigits value="4" />
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="MonetaryAmountType">
		<xsd:annotation>
			<xsd:documentation>Defines a monetary amount.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<!-- decimal(13,4) -->
			<xsd:fractionDigits value="4" />
			<xsd:totalDigits value="17" />
		</xsd:restriction>
	</xsd:simpleType>

</xsd:schema>
